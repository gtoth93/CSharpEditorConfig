#   CSharpier: Unsupported, spaces are removed between the method call name and the opening 
#     parenthesis.
# Tags: @formatting, @rider, @space, @vs
csharp_space_between_method_call_name_and_opening_parenthesis = false

# Description: Sets whether there should be spaces after commas.
# Possible values: true, false
# Behavior:
#   Visual Studio: When false, spaces are removed after commas. When true, spaces are added after 
