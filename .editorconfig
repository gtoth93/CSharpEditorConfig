# Tags: @formatting, @rider, @space, @vs
csharp_space_after_dot = false

# Description: Sets whether there should be spaces before dots.
# Possible values: true, false
# Behavior:
#   Visual Studio: When false, spaces are removed before dots. When true, spaces are added before dots.
#   Rider: When false, spaces are removed before dots. When true, if csharp_space_after_dot = false, 
#     then spaces are removed before dots, if csharp_space_after_dot = true, then spaces are added 
#     before and after dots.
#   CSharpier: Unsupported, spaces are removed before dots.
# Tags: @formatting, @rider, @space, @vs
csharp_space_before_dot = false

# Description: Sets whether there should be a space after the semicolons in for statements.
# Possible values: true, false
# Behavior:
#   Visual Studio: When false, spaces are removed after the semicolons in for statements. When true,
#     spaces are added after the semicolons in for statements.
#   Rider: When false, spaces are removed after the semicolons in for statements. When true, spaces
#     are added after the semicolons in for statements.
#   CSharpier: Unsupported, spaces are added after the semicolons in for statements.
# Tags: @formatting, @rider, @space, @vs
csharp_space_after_semicolon_in_for_statement = true

# Description: Sets whether there should be a space before the semicolons in for statements.
# Possible values: true, false
# Behavior:
#   Visual Studio: When false, spaces are removed before the semicolons in for statements. When true,
#     spaces are added before the semicolons in for statements.
#   Rider: When false, spaces are removed before the semicolons in for statements. When true, spaces
#     are added before the semicolons in for statements.
#   CSharpier: Unsupported, spaces are removed before the semicolons in for statements.
# Tags: @formatting, @rider, @space, @vs
csharp_space_before_semicolon_in_for_statement = false

# Description: Sets whether extra spaces should be removed from declaration statements.
# Possible values: false, ignore
# Behavior:
#   Visual Studio: When false, extra spaces are removed from declaration statements. When ignore, 
#     extra spaces are not removed from declaration statements.
#   Rider: Unsupported, uses csharp_space_around_binary_operators to add or remove spaces around 
#     declaration statements, and resharper_extra_spaces controls whether to remove extra spaces.
#   CSharpier: Unsupported, removes extra spaces from declaration statements.
# Tags: @formatting, @space, @vs
csharp_space_around_declaration_statements = false

# Description: Sets whether there should be a space before opening square brackets.
# Possible values: true, false
# Behavior:
#   Visual Studio: When false, spaces are removed before opening square brackets. When true, a space
#     is added before opening square brackets.
#   Rider: When false, spaces are removed before opening square brackets. When true, a space is added
#     before opening square brackets.
#   CSharpier: Unsupported, spaces are removed before opening square brackets.
# Tags: @formatting, @rider, @space, @vs
csharp_space_before_open_square_brackets = false

# Description: Sets whether there should be a space between empty square brackets.
# Possible values: true, false
# Behavior:
#   Visual Studio: When false, spaces are removed between the square brackets. When true, a space is
#     added between square brackets.
#   Rider: When false, spaces are removed between the square brackets. When true, a space is added 
#     between square brackets.
#   Csharpier: Unsupported, spaces are removed between the square brackets.
# Tags: @formatting, @rider, @space, @vs
csharp_space_between_empty_square_brackets = false

# Description: Sets whether there should be spaces between square brackets and their content (index, 
#   list items, ...).
# Possible values: true, false
# Behavior:
#   Visual Studio: When false, spaces are removed between the square brackets and their content. When
#     true, spaces are added between the square brackets and their content.
#   Rider: When false, spaces are removed between the square brackets and their content. When true, 
#     spaces are added between the square brackets and their content.
#   CSharpier: Unsupported, spaces are removed between the square brackets and their content.
# Tags: @formatting, @rider, @space, @vs
csharp_space_between_square_brackets = false

# Description: Sets whether statements and member declaration should be left on the same line.
# Possible values: true, false
# Behavior:
#   Visual Studio: When false, statements on the same line are moved to separate lines.. When true, 
#     statements on the same line are kept on the same line. When switching back to false, statements
#     on separate lines are not collapsed to a single line.
#   Rider: Unsupported, statements on the same line are moved to separate lines.
#   CSharpier: Unsupported, statements on the same line are moved to separate lines.
# Tags: @formatting, @vs, @wrapping
csharp_preserve_single_line_statements = false

# Description: Sets whether single line blocks should be kept on the same line.
# Possible values: true, false
# Behavior:
#   Visual Studio: When false, single line blocks are expanded. When true, single line blocks are 
#     kept on the same line. When switching back to false, multiline blocks are not collapsed to a 
#     single line.
#   Rider: When false, single line blocks are expanded. When true, single line blocks are kept on 
#     the same line. When switching back to false, multiline blocks are not collapsed to a single 
#     line.
#   CSharpier: Unsupported, single line blocks are collapsed to a single line if the block is not 
#     longer than max_line_length. Auto property blocks are always collapsed to a single line.
# Tags: @formatting, @rider, @wrapping, @vs
csharp_preserve_single_line_blocks = true

#### ReSharper rules: Blank Lines ####

# For Visual Studio, the closest equivalent rule for most of these ReSharper rules is 
# dotnet_style_allow_multiple_blank_lines_experimental, if it is set to false, then the rule removes 
# consecutive blank lines on code cleanup (needs the 'Apply blank line preferences (experimental)' 
# fixer), which would be setting the ReSharper rules to 1. If there are no blank lines between code 
# elements, then no blank lines are added, it has to be added manually. The behavior section won't 
# include Visual Studio, because it is the same behavior for all of these rules.

# Description: Sets how many blank lines should be before and after a region.
# Possible values: an integer (usually in the range of 0-2)
# Behavior:
#   Rider: Adds or removes blank lines before and after region, if resharper_keep_blank_lines_in_declarations 
#     is higher than resharper_blank_lines_around_region, then it will override this rule when
#     removing blank lines.
#   CSharpier: If there are multiple blank lines before the region, one blank line is kept. If there
#     are multiple blank lines after the region, two blank lines are kept. If there are no blank 
#     lines before the region, one blank line is added. If there are no blank lines after the region,
#     no blank lines are added.
# Tags: @blank-line, @formatting, @newline, @rider
resharper_blank_lines_around_region = 1

# Description: Sets how many blank lines should be inside a region.
# Possible values: an integer (usually in the range of 0-2)
# Behavior:
#   Rider: Adds or removes blank lines inside the region.
#   CSharpier: If there are multiple blank lines after the region preprocessor directive, two blank 
#     lines are kept. If there are multiple blank lines before the endregion preprocessor directive, 
#     one blank line is kept. If there are no blank lines after the region or before the endregion, 
#     no blank lines are added.
# Tags: @blank-line, @formatting, @newline, @rider
resharper_blank_lines_inside_region = 1

# Description: Sets how many blank lines should be before a single line comment.
# Possible values: an integer (usually in the range of 0-2)
# Behavior:
#   Rider: Adds or removes blank lines before single line comments. If resharper_keep_blank_lines_in_declarations
#     or resharper_keep_blank_lines_in_code is higher than resharper_blank_lines_before_single_line_comment,
#     then they will override this rule when removing blank lines.
#   CSharpier: If there multiple multiple blank lines before the comment, then one blank line is kept.
#     If there is no blank line before the comment, then no blank line is added.
# Tags: @blank-line, @formatting, @newline, @rider
resharper_blank_lines_before_single_line_comment = 0
resharper_keep_blank_lines_in_declarations = 1
